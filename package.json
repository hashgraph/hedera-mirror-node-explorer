{
  "name": "hedera-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "cover:unit": "vitest run --coverage",
    "test:e2e": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test dev https://localhost:5173 'cypress run --browser chrome --e2e'",
    "test:e2e:dev": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'vite dev --port 5173' https://localhost:5173 'cypress open --e2e'",
    "build-only": "npm run install:branding && vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "install:branding": ". ./src/assets/branding/install-branding.sh",
    "docker:build": "npm run build && docker build . --tag gcr.io/hedera-registry/hedera-mirror-node-explorer",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@bladelabs/blade-web3.js": "1.0.0",
    "@ethereum-sourcify/bytecode-utils": "^1.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@hashgraph/proto": "^2.12.0",
    "@hashgraph/sdk": "^2.27.0",
    "@kabuto-sh/ns": "^0.7.0",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/providers": "^9.1.0",
    "@oruga-ui/oruga-next": "^0.6.0",
    "axios": "^0.27.2",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.2.0",
    "bulma": "^0.9.4",
    "core-js": "^3.30.1",
    "ethers": "^5.7.2",
    "hashconnect": "^0.2.4",
    "vue": "^3.3.4",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "axios-mock-adapter": "^1.21.4",
    "cypress": "~12.14.0",
    "esbuild": "^0.18.11",
    "eslint": "^8.39.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "resize-observer-polyfill": "~1.5.1",
    "start-server-and-test": "^2.0.0",
    "sass": "^1.62.1",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vue-tsc": "^1.6.5"
  }
}
