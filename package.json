{
  "name": "hedera-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run install:branding && vite",
    "build": "run-p type-check build-only",
    "preview": "npm run install:branding && vite preview",
    "test:unit": "vitest run --threads false",
    "cover:unit": "vitest run --coverage --threads false",
    "test:e2e": "npm run install:branding && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test dev https://localhost:5173 'cypress run --browser chrome --e2e'",
    "test:e2e:dev": "npm run install:branding && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'vite dev --port 5173' https://localhost:5173 'cypress open --e2e'",
    "build-only": "npm run install:branding && vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "install:branding": ". ./src/assets/branding/install-branding.sh",
    "docker:build": "npm run build && docker build . --tag gcr.io/hedera-registry/hedera-mirror-node-explorer",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@bladelabs/blade-web3.js": "1.1.1",
    "@ethereum-sourcify/bytecode-utils": "^1.2.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@hashgraph/proto": "^2.13.0",
    "@hashgraph/sdk": "^2.39.0",
    "@kabuto-sh/ns": "^0.12.0",
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^13.1.0",
    "@oruga-ui/oruga-next": "^0.7.0",
    "axios": "^1.6.2",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.2.0",
    "bulma": "^0.9.4",
    "core-js": "^3.34.0",
    "crypto-js": "4.1.1",
    "ethers": "^5.7.2",
    "hashconnect": "^0.1.10",
    "solc": "^0.8.23",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "web3-utils": "1.10.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.13",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/web-worker": "^0.34.7",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "~13.2.0",
    "esbuild": "^0.18.20",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "resize-observer-polyfill": "~1.5.1",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.0.4",
    "vite": "^4.5.1",
    "vite-plugin-node-polyfills": "^0.9.0",
    "vite-plugin-require-transform": "^1.0.21",
    "vite-plugin-rewrite-all": "1.0.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.25"
  }
}
