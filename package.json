{
  "name": "@hashgraph/hedera-explorer",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run install:branding && vite --host",
    "dev:http": "npm run install:branding && cp networks-config-http-example.json ./public/networks-config.json  && vite --config nossl.config.ts",
    "build": "run-p type-check build-only",
    "clean": "rm -rf dist",
    "preview": "npm run install:branding && vite preview",
    "test:unit": "vitest run",
    "cover:unit": "vitest run --coverage",
    "test:e2e": "npm run install:branding && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test dev https://localhost:5173 'cypress run --browser chrome --e2e'",
    "test:e2e:dev": "npm run install:branding && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'vite dev --port 5173' https://localhost:5173 'cypress open --e2e'",
    "build-only": "npm run install:branding && vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "install:branding": ". ./src/assets/branding/install-branding.sh",
    "docker:build": "npm run build && docker build . --tag gcr.io/hedera-registry/hedera-mirror-node-explorer",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "sourcify:check-setup": "bash ./scripts/check-hosts-setup.sh",
    "sourcify:start": "npm run sourcify:check-setup && docker compose -f ./sourcify-setup/docker-compose.yaml up -d",
    "sourcify:stop": "docker compose -f ./sourcify-setup/docker-compose.yaml down"
  },
  "dependencies": {
    "@bokuweb/zstd-wasm": "^0.0.27",
    "@ethereum-sourcify/bytecode-utils": "^1.3.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@hashgraph/proto": "^2.15.0",
    "@hashgraph/sdk": "^2.59.0",
    "@hedera-name-service/hns-resolution-sdk": "^2.0.15",
    "@kabuto-sh/ns": "^0.14.2",
    "@oruga-ui/oruga-next": "^0.9.3",
    "@oruga-ui/theme-oruga": "^0.5.1",
    "@vuepic/vue-datepicker": "^11.0.1",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/sign-client": "^2.19.0",
    "@walletconnect/types": "^2.16.1",
    "axios": "^1.8.1",
    "base32-decode": "^1.0.0",
    "base32-encode": "^2.0.0",
    "chart.js": "^4.4.8",
    "ethers": "^6.13.5",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lucide-vue-next": "^0.476.0",
    "multiformats": "^13.3.2",
    "prismjs": "^1.29.0",
    "vue": "^3.5.11",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.12",
    "@rushstack/eslint-patch": "^1.10.5",
    "@tsconfig/node18": "^18.2.4",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.8",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/web-worker": "^3.0.7",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "axios-mock-adapter": "^2.1.0",
    "cypress": "^14.1.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "resize-observer-polyfill": "~1.5.1",
    "sass": "^1.85.1",
    "start-server-and-test": "^2.0.10",
    "typescript": "~5.7.3",
    "vite": "^6.2.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-require-transform": "^1.0.21",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.8"
  },
  "overrides": {
    "elliptic": "^6.6.1"
  }
}
